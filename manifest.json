{
  "name": "Unicode Extensions",
  "version": "1.0",
  "description": "Basic unicode extension for Browsers.",
  "author": "Bieefilled",
  "manifest_version": 3,

  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "./styles/images/diamonds.png",
    "32": "./styles/images/diamonds.png",
    "48": "./styles/images/diamonds.png",
    "128": "./styles/images/diamonds.png"
  },
  "action": {
    "default_icon": {
      "16": "./styles/images/diamonds.png",
      "32": "./styles/images/diamonds.png",
      "48": "./styles/images/diamonds.png",
      "128": "./styles/images/diamonds.png"
    },
    "default_title": "Unicode Extensions",
    "default_popup": "index.html",
    "background": {
      "service_worker": "index.js"
    }
  }
}
