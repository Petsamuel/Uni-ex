{
  "name": "Unicode Extensions",
  "version": "1.0",
  "description": "Basic unicode extension for Browsers.",
  "author": "Bieefilled",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"]
      //   "js": ["../src/content.tsx"]
    }
  ],

  "permissions": ["activeTab", "declarativeContent", "storage"],

  "action": {
    "default_icon": {
      "16": "vite.svg",
      "32": "vite.svg",
      "48": "vite.svg",
      "128": "vite.svg"
    },
    "default_title": "Unicode Extensions",
    "default_popup": "index.html"
  }
}
