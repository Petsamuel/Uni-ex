{
  "name": "Unicode Extensions",
  "version": "1.0",
  "description": "Basic unicode extension for Browsers.",
  "author": "Bieefilled",
  "manifest_version": 3,
  //   "background": {
  //     "service_worker": "../src/content.tsx",
  //     "type": "module"
  //   },
  "hosted_permission": ["https://*/*", "http://*/*", "localhost:*/*"],

  "permissions": ["activeTab", "scripting"],

  "action": {
    "default_icon": {
      "16": "vite.svg",
      "32": "vite.svg",
      "48": "vite.svg",
      "128": "vite.svg"
    },
    "default_title": "Unicode Extensions",
    "default_popup": "index.html"
  }
}
