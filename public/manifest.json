{
  "name": "Unicode Extensions",
  "version": "1.0",
  "description": "Basic unicode extension for Browsers.",
  "author": "Bieefilled",
  "manifest_version": 3,

  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "diamonds.png",
    "32": "diamonds.png",
    "48": "diamonds.png",
    "128": "diamonds.png"
  },
  "action": {
    "default_icon": {
      "16": "diamonds.png",
      "32": "diamonds.png",
      "48": "diamonds.png",
      "128": "diamonds.png"
    },
    "default_title": "Unicode Extensions",
    "default_popup": "index.html",
    "background": {
      "service_worker": "../src/service-worker.ts"
    }
  }
}
